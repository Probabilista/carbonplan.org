{
  "version": 2,
  "routes": [
    {
      "src": "/reports/_next(.*)",
      "dest": "https://reports.carbonplan.org/_next$1"
    },
    {
      "src": "/reports(.*)",
      "dest": "https://reports.carbonplan.org/reports$1"
    },
    {
      "src": "/research/_next(.*)",
      "dest": "https://research.carbonplan.org/_next$1"
    },
    {
      "src": "/research(.*)",
      "dest": "https://research.carbonplan.org/research$1"
    },
    {
      "src": "/jobs",
      "status": 307,
      "headers": {
        "Location": "https://apply.workable.com/carbonplan/"
      }
    }
  ]
}
